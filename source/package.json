{
	"name": "@@name@@",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"typecheck": "tsc --noEmit",
		"lint": "npx eslint src/**/*.ts",
		"lint:fix": "npx eslint --fix src/**/*.ts",
		"dev": "aerofoil port-runner \"npx wrangler dev --config ./dist/wrangler.toml --port {port}\"",
		"build": "tsx ./build.ts & tsc --emitDeclarationOnly --declaration & tsc-alias",
		"preview": "aerofoil port-runner \"npx wrangler dev --config ./dist/wrangler.toml --port {port}\"",
		"start": "aerofoil no-op",
		"deploy:production": "aerofoil deploy --local-package"
	},
	"type": "module",
	"types": "./dist/index.d.ts",
	"exports": {
		"./package.json": "./package.json",
		".": {
			"types": "./dist/index.d.ts"
		}
	},
	"devDependencies": {
		"@aerofoil/aerofoil-core": "latest",
		"@aerofoil/af-ext-cloudflare": "latest",
		"@cloudflare/workers-types": "latest",
		"eslint": "latest",
		"eslint-config-aerofoil": "workspace:*",
		"tsc-alias": "latest",
		"tsconfig": "workspace:*",
		"tsx": "latest",
		"type-fest": "latest",
		"typescript": "latest",
		"wrangler": "latest",
		"zod": "v3.22.2"
	},
	"dependencies": {
		"nanoid": "^4.0.2"
	},
	"eslintConfig": {
		"extends": [
			"aerofoil/base"
		],
		"parserOptions": {
			"sourceType": "module",
			"project": "./tsconfig.json"
		},
		"settings": {
			"import/resolver": {
				"typescript": {
					"alwaysTryTypes": true,
					"project": "./tsconfig.json"
				}
			}
		}
	}
}